{
  "name": "mochawesome-report",
  "version": "1.0.0-alpha.1",
  "description": "Generates gorgeous HTML reports in Mocha.js from mochawesome reporter.",
  "scripts": {
    "lint": "eslint bin lib src test/spec",
    "test": "cross-env UV_THREADPOOL_SIZE=100 BABEL_DISABLE_CACHE=1 NODE_ENV=test nyc $(npm bin)/mocha --require test/helper.js \"test/spec/**/*.test.+(js|jsx)\"",
    "test:single": "cross-env UV_THREADPOOL_SIZE=100 BABEL_DISABLE_CACHE=1 NODE_ENV=test nyc $(npm bin)/mocha --require test/helper.js ",
    "test:fn": "node ./test-functional/create-report.js",
    "start": "node ./lib/main.js",
    "compile:main": "babel ./lib/main.js --out-dir dist",
    "build:main": "$(npm bin)/webpack --config ./webpack/main.config.js",
    "build:main:prod": "NODE_ENV=production $(npm bin)/webpack --config ./webpack/main.config.js",
    "build:assets": "rm -rf dist/assets && npm run pack && npm run pack:inline",
    "build:assets:prod": "NODE_ENV=production npm run build:assets",
    "pack": "$(npm bin)/webpack --config ./webpack/external.config.js",
    "pack:inline": "$(npm bin)/webpack --config ./webpack/inline.config.js",
    "devserver": "$(npm bin)/webpack-dev-server --config ./webpack/dev.config.js --inline --watch --hot",
    "cli": "node ./bin/cli test/sample-data/test-data.json --dev",
    "dev": "npm run cli && npm run devserver"
  },
  "author": "Adam Gruber",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/adamgruber/mochawesome-report"
  },
  "keywords": [
    "mochawesome",
    "mocha",
    "reporter",
    "json",
    "react"
  ],
  "engine": "node >= 4",
  "main": "dist/main.js",
  "bin": {
    "mareport": "./bin/cli.js"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "html",
      "text",
      "cobertura"
    ],
    "extension": [
      ".js",
      ".jsx"
    ],
    "exclude": [
      "test/**/*",
      "src/components/summary",
      "src/components/status-bar"
    ],
    "check-coverage": true,
    "statements": 95,
    "branches": 90,
    "functions": 95,
    "lines": 95
  },
  "dependencies": {
    "chart.js": "^2.3.0",
    "classnames": "^2.2.5",
    "fs-extra": "^1.0.0",
    "highlight.js": "^9.7.0",
    "json-loader": "^0.5.4",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.16.4",
    "material-design-icons": "^3.0.1",
    "mobx": "^2.6.0",
    "mobx-react": "^3.5.8",
    "mobx-react-devtools": "^4.2.9",
    "mocha": "*",
    "moment": "^2.15.1",
    "ncp": "^2.0.0",
    "normalize.css": "^5.0.0",
    "postcss": "^5.2.5",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "yargs": "^6.3.0"
  },
  "devDependencies": {
    "app-module-path": "^2.1.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "bootstrap": "^3.3.7",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "css-modules-require-hook": "^4.0.5",
    "enzyme": "^2.5.1",
    "eslint": "^3.10.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jsdom": "^9.8.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "nyc": "^8.3.2",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^1.0.0",
    "postcss-reporter": "^1.4.1",
    "postcss-url": "^5.1.2",
    "proxyquire": "^1.7.10",
    "should": "^11.1.1",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "stylelint": "^7.5.0",
    "stylelint-config-standard": "^14.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
